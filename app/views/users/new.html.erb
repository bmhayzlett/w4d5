<!--NEW USER-->
<form action="<%= users_url(@user) %>" method = "post">
  <input
   type="hidden"
   name="authenticity_token"
   value="<%= form_authenticity_token %>">

  <p>Username</p>
  <p><input type="text" name="user[username]" value="<%=@user.username%>"/></p>

  <p>Password</p>
  <p><input type="password" name="user[password]"/></p>

  <button type="submit">Sign Up!</button>

</form>
