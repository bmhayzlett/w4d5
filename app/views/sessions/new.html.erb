<form action="<%= session_url %>" method = "post">
  <input
   type="hidden"
   name="authenticity_token"
   value="<%= form_authenticity_token %>">

  <p>Username</p>
  <p><input type="text" name="user[username]"/></p>

  <p>Password</p>
  <p><input type="password" name="user[password]"/></p>

  <button type="submit">Log In!</button>

</form>
